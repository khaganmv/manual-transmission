<?xml version="1.0"?>
<bindings>
	<mapping name="Upshift_Button" type="Button" >
		<button id="IK_Pad_DigitLeft" />
	</mapping>

    <mapping name="Downshift_Button" type="Button" >
		<button id="IK_Pad_DigitDown" />
	</mapping>

    <context name="VehicleDriveBase" append="true">
		<action name="Upshift" map="Upshift_Button" />
		<action name="Downshift" map="Downshift_Button" />
    </context>
</bindings>
